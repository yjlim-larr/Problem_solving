#include<iostream>
using namespace std;

int T;
int info[12][12];
bool table[12];
int ret = 0;

void dfs(int prev, int s) {
	if (prev == 12) { //ë.
		ret = ret < s ? s : ret;
		return;
	}

	int ret = 0;
	for (int i = 1; i <= 11; i++) {
		if (table[i] == 0) {
			table[i] = 1;

			if (info[i][prev] != 0) 
				dfs(prev + 1, s + info[i][prev]);
			
			table[i] = 0;
		}
	}

	return;
}

int main() {
	cin >> T;

	for (int k = 1; k <= T; k++) {
		for (int i = 1; i <= 11; i++) 
			for (int j = 1; j <= 11; j++) 
				cin >> info[i][j];

		dfs(1, 0);  
		cout << ret << '\n';
		ret = 0;
	}

	return 0;
}
